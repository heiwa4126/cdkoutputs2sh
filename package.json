{
	"name": "cdkoutputs2sh",
	"version": "1.0.0",
	"description": "tsup test",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"bin": {
		"cdkoutputs2sh": "dist/main.js"
	},
	"scripts": {
		"init": "pnpm i && pnpm run build && pnpm run link",
		"build": "tsup",
		"link": "mkdir -p node_modules && ln -sfn ../dist node_modules/cdkoutputs2sh",
		"test": "vitest run",
		"test:run": "vitest run",
		"test:watch": "vitest --watch",
		"lint": "biome lint .",
		"lint:full": "biome lint . --fix && shopt -s globstar && tsc --noEmit src/**/*.ts",
		"format": "biome format --write .",
		"format:full": "npm pkg fix && biome format --write . && dprint fmt && textlint --fix \"**/*.md\"",
		"ex1": "node ./example/ex1.mjs && tsx ./example/ex1.ts",
		"smoketest": "npm run ex1"
	},
	"keywords": [
		"hello-world",
		"example",
		"demo",
		"typescript",
		"esm",
		"commonjs",
		"cli",
		"tsup",
		"learning",
		"sigstore",
		"trusted-publishing",
		"github-actions"
	],
	"author": "heiwa4126 <heiwa4126@gmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/heiwa4126/cdkoutputs2sh.git"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.18.3",
	"devDependencies": {
		"@types/node": "^24.9.1",
		"tsup": "^8.5.0",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	}
}
